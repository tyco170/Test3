.PROGRAM rob.main_0()
;
; ABSTRACT:     Main robot application program
;
; INPUTS:       None
;
; OUTPUTS:      None
;
        GLOBAL REAL rob.run[]

; Initialize error reporting


; Main robot processing loop

        WHILE rob.run[0] DO
            CALL rob.pick_0()
            CALL rob.place_0()
        END

        CALL rob.move.safe_0()

        RETURN
.END

